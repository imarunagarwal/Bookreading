@{
    if(Session["UserID"]!=null && (int)Session["UserID"]==1)
    {
        Layout = "~/Views/Shared/_Layout3.cshtml";
    }
    else if (Session["UserID"] != null)
    {
        Layout = "~/Views/Shared/_Layout2.cshtml";
    }
    else
    {
        Layout = "~/Views/Shared/_Layout.cshtml";
    }
}
